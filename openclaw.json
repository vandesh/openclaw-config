      "subagents": {
        "maxConcurrent": 8
      },
      "sandbox": {
        "mode": "off",
        "workspaceAccess": "ro"
      }
    }
  },
  "tools": {
    "web": {
      "search": {
        "enabled": true,
        "apiKey": "${BRAVE_API_KEY}"
      },
      "fetch": {
        "enabled": true
      }
    },
    "exec": {
      "security": "allowlist",
      "safeBins": [
        "git",
        "npm",
        "curl",
        "ls",
        "ps",
        "kill",
        "pkill"
      ]
